    <template id="USER_TYPE" protocol="MS" region="南网">
        <name>用户类型</name>
        <length>1</length>
        <type>BIN</type>
    </template>
    <template id="ADRESS_TYPE" protocol="MS" region="南网">
        <name>用户地址</name>
        <length>6</length>
        <type>NORMAL</type>
    </template>
    <template id="RANGE_OF_USER_TYPE" protocol="MS" region="南网">
        <name>用户类型区间</name>
        <length>3</length>
        <splitByLength>
            <name>区间类型</name>
            <length>1</length>
            <type>BIN</type>
            <value key = "00">前闭后开</value>
            <value key = "01">前开后闭</value>
            <value key = "02">前闭后闭</value>
            <value key = "03">前开后开</value>
            <value key="other">未知</value>
        </splitByLength>
        <splitByLength>
            <name>用户类型</name>
            <length>1</length>
            <type>BIN</type>
        </splitByLength>
        <splitByLength>
            <name>用户类型</name>
            <length>1</length>
            <type>BIN</type>
        </splitByLength>
    </template>
    <template id="METER_TYPE" protocol="MS" region="南网">
        <name>电能表类型</name>
        <length>1</length>
        <type>BIN</type>
    </template>
    <template id="RANGE_OF_NUMBER" protocol="MS" region="南网">
        <name>配置序号区间</name>
        <splitByLength>
            <name>区间类型</name>
            <length>1</length>
            <type>BIN</type>
            <value key = "00">前闭后开</value>
            <value key = "01">前开后闭</value>
            <value key = "02">前闭后闭</value>
            <value key = "03">前开后开</value>
            <value key="other">未知</value>
        </splitByLength>
        <splitByLength>
            <name>数据类型</name>
            <length>1</length>
            <type>BIN</type>
        </splitByLength>
        <splitByLength>
            <name>测量点号</name>
            <length>2</length>
            <type>BIN_BE</type>
        </splitByLength>
        <splitByLength>
            <name>数据类型</name>
            <length>1</length>
            <type>BIN</type>
        </splitByLength>
        <splitByLength>
            <name>测量点号</name>
            <length>2</length>
            <type>BIN_BE</type>
        </splitByLength>
    </template>
    <template id="EVENT_LEVEL" protocol="MS" region="南网">
        <name>事件等级</name>
        <length>1</length>
        <type>BIN</type>
    </template>
    <template id="PORT_TYPE" protocol="MS" region="南网">
        <name>端口</name>
        <length>1</length>
        <type>NORMAL</type>
        <value key="00">交采</value>
        <value key="01">485_1</value>
        <value key="02">485_2</value>
        <value key="03">485_3</value>
        <value key="04">485_4</value>
        <value key="05">485_5</value>
        <value key="06">485_6</value>
        <value key="07">485_7</value>
        <value key="08">485_8</value>
        <value key="1F">载波</value>
        <value key="other">未知</value>
    </template>
</config>