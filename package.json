    "daisyui": "^5.5.19",
    "html-to-image": "^1.11.13",
    "lucide-react": "^0.575.0",
    "modern-screenshot": "^4.6.8",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hot-toast": "^2.6.0",
    "react-redux": "^9.2.0",
    "react-resizable-panels": "^4.6.5",
    "react-router-dom": "^7.13.1",
    "tailwindcss": "^4.2.1",
    "zustand": "^5.0.11"
