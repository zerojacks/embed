{
  "name": "embedtool",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:wasm && tsc -b && vite build",
    "build:frontend": "tsc -b && vite build",
    "build:wasm": "powershell -ExecutionPolicy Bypass -File build_wasm.ps1",
